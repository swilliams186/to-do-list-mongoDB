
    <%-include("header")-%>
    <div class="box" id="heading">
        <h1>
            <%= listTitle %>
        </h1>
    </div>
    <div class="box">
        <% items.forEach(function(item){ %>
            <div class="item">
                <input type="checkbox">
                <p>
                    <%= item.text %>
                </p>
            </div>
            <% }) %>

            <form class="item" action="<%=postAction%>" method="post">
                <input type="text" name="newToDo">
                <button type="submit">+</button>
            </form>

    </div>

    <%-include("footer")-%>